{
    "LLM-Clients": {
        "DefaultProvider": "qwen3-vl-plus",
        "Defaults": {
            "TimeoutSeconds": 60,
            "MaxRetryCount": 2,
            "EnableStreaming": true,
            "SupportsFunctionCalling": true,
            "SupportsJsonSchema": true,
            "SupportsVision": false,
            "SupportsThinking": false,
            "MaxContextTokens": 4096,
            "Pipeline": {
                // ===== 第一层：基础设施 =====
                "EnableRetry": true,
                "EnableRateLimiting": true,
                // ===== 第二层：可观测性 =====
                "EnableLogging": true,
                "EnableAudit": true,
                "EnableRAGTracing": false,
                // ===== 第三层：业务处理 =====
                "EnableBoundaryCheck": false,
                "EnableInstruction": false,
                "EnableCaching": false,
                "CacheDurationMinutes": 5,
                // ===== 第四层：成本控制 =====
                "EnableTokenMonitoring": true,
                // ===== 第五层：上下文管理 =====
                // 实际压缩配置见独立的 CompressionConfig (LLMAgent.Features.json)
                "EnableChatReducer": false,
                "ChatReducerType": "MessageCounting",
                // ===== 第六层：工具处理 =====
                "EnableToolValidation": false,
                "EnableToolMonitoring": false,
                "EnableToolReduction": false,
                "ToolLimit": 5,
                // ===== Function Calling =====
                "EnableFunctionInvocation": true,
                "AllowConcurrentInvocation": false,
                "IncludeDetailedErrors": true,
                "MaxIterationsPerRequest": 10
            }
        },
        "Clients": {
            "DeepSeek": {
                "ApiKey": "sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx",
                "ModelId": "deepseek-chat",
                "BaseUrl": "https://api.deepseek.com/v1",
                "InPrice": 2,
                "OutPrice": 3
            },
            "DeepSeek-R1": {
                "ApiKey": "sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx",
                "ModelId": "deepseek-reasoner",
                "BaseUrl": "https://api.deepseek.com/v1",
                // 思考模式配置（仅支持思考的模型需要配置，如 DeepSeek-R1, o1, Claude 3.5 等）
                "SupportsThinking": true,
                "Thinking": {
                    "EnableByDefault": true, // 默认是否启用思考模式
                    "ShowThinkingProcess": true, // 是否在响应中显示思考过程
                    "ThinkingParamName": "reasoning_content" // DeepSeek 的思考内容字段名
                },
                "InPrice": 4,
                "OutPrice": 16
            },
            "SiliconFlow": {
